package utils;

import java.util.ArrayList;
import java.util.Scanner;

public class GetArrayListInput {
	static ArrayList<Integer> arr = new ArrayList<>() ;
	
	public static ArrayList<Integer> getList(){
		arr.clear();
		Scanner sc = new Scanner(System.in);
		while(sc.hasNextInt()){
			arr.add(sc.nextInt());
		}
		return arr;
	}
}
